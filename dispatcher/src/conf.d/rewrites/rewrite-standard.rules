#
# This file contains the rewrite rules, and can be customized.
#
# By default, it includes just the rewrite rules. You can
# add rewrite rules to this file but you should still include
# the default rewrite rules.
Include conf.d/rewrites/default_rewrite.rules


## rewrite rule(s)
# RewriteEngine On

# BLOCK the 'ltx71' thing thats been giving us grief
RewriteCond %{HTTP_USER_AGENT} ^.*ltx71.*$
RewriteRule . - [R=403,L,NC]

ErrorDocument 404 /en-global/page-not-found

RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1$1 [R=301,L]


### Send all requests to '/discover/' to the path without that prefix
RewriteRule ^/discover/etc.clientlibs/dhl/(.*?)$ /etc.clientlibs/dhl/$1 [NC,PT,L]
RewriteRule ^/discover/apps/dhl/discoverdhlapi/(.*?)$ /apps/dhl/discoverdhlapi/$1 [NC,PT,L]
RewriteRule ^/discover/(.*?)$ /$1 [R=302,L]
RewriteRule ^/discover$ / [R=302,L]


### Rewrite all requests from / to welcome page
RewriteRule ^/content/dhl.html$ https://%{HTTP_HOST} [R=301,L]
RewriteRule ^/content/dhl/(.*)$ https://%{HTTP_HOST}/$1 [R=301,L]


