{
  "name": "discover-dhl-front-end-module",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": ""
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "webpack-build": "webpack --config ./webpack.dev.js",
    "dev": "webpack --env dev --config ./webpack.dev.js && npm run dependencies && clientlib --verbose",
    "prod": "webpack --config ./webpack.prod.js && npm run dependencies:prod && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "eslint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx src",
    "eslint:fix": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx src --fix",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "dependencies": "webpack --config ./webpack.dependency.js",
    "dependencies:prod": "webpack --config ./webpack.dependency.prod.js",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env writeToDisk & chokidar -c \"clientlib\" ./dist & aemsync -w ../ui.apps/src/main/content"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/css-modules": "^1.0.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.4.3",
    "aemsync": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.2.1",
    "chokidar-cli": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^10.1.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "cssnano": "^5.0.12",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.1.1",
    "glob": "^10.3.10",
    "glob-import-loader": "^1.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.1.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.14.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^5.3.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "dompurify": "^3.1.6",
    "react-dom": "^18.2.0",
    "react-select": "^5.8.0",
    "react-string-format": "^1.0.1",
    "usehooks-ts": "^2.9.5"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
