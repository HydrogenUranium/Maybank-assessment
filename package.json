{
  "devDependencies": {
    "aemsync": "^5.0.5"
  },
  "scripts": {
    "aemsync": "aemsync -t http://admin:admin@localhost:4502 -w ./ui.apps/src/main/content",
    "aemsync:publish": "aemsync -t http://admin:admin@localhost:4503 -w ./ui.apps/src/main/content",
    "cypress": "cd ./ui.tests/test-module && npm run open",
    "build:frontend": "cd ./ui.frontend && npm run dev",
    "build:css": "cd ./ui.apps && npm run build:css",
    "build:js": "cd ./ui.apps && npm run build:js",

    "rde:deploy:apps": "aio aem:rde:install ui.apps/target/discover.ui.apps-2.00.0-SNAPSHOT.zip",
    "rde:deploy:all": "aio aem:rde:install all/target/discover.all-2.00.0-SNAPSHOT.zip",
    "rde:deploy:config": "aio aem:rde:install ui.config/target/discover.ui.config-2.00.0-SNAPSHOT.zip",
    "rde:deploy:dispatcher": "aio aem:rde:install dispatcher/target/discover.dispatcher.cloud-2.00.0-SNAPSHOT.zip",
    "rde:build-and-deploy:apps": "cd ui.apps && mvn clean install -PautoInstallPackage && cd .. && npm run rde:deploy:apps"
  }
}
