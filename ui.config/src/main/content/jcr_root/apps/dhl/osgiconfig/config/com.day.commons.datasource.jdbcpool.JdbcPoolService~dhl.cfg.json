{
    "jdbc.driver.class": "com.mysql.jdbc.Driver",
    "jdbc.connection.uri": "jdbc:mysql://$[env:AEM_PROXY_HOST;default=proxy.tunnel]:$[env:MYSQL_PORT;default=30006]/$[env:MYSQL_CONNECTION_DATABASE]?useLegacyDatetimeCode=false&serverTimezone=CET&autoReconnect=true&enabledTLSProtocols=TLSv1.2",
    "jdbc.username": "$[env:MYSQL_CONNECTION_USERNAME]",
    "jdbc.password": "$[secret:MYSQL_CONNECTION_PASSWORD]",
    "jdbc.validation.query": "",
    "default.readonly": "false",
    "default.autocommit": "true",
    "pool.size": "50",
    "pool.max.wait.msec": "3000",
    "datasource.name": "dhlmysql",
    "datasource.svc.properties": []
}