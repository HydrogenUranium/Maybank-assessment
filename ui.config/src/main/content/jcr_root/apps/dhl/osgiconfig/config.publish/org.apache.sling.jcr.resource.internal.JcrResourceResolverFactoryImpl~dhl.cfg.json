{
    "resource.resolver.mapping": [
        "/:/discover/",
        "/content/dhl.html:/discover",
        "/content/dhl/(global|\\w{2})/(.*)</discover/$2",
        "/content/dam/(.*)</discover/content/dam/$1",
        "/adobe/dynamicmedia/deliver/(.*)</discover/adobe/dynamicmedia/deliver/$1",
        "/content/dhl:/discover",
        "/content/dhl.sitemap.sitemap.xml:/discover/sitemap.xml",
        "/content/dhl.sitemap.(global|\\w{2})-sitemap.xml:/discover/$1/sitemap.xml"
    ],
    "resource.resolver.map.location": "/etc/map.publish"
}