{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "services/collector/{type:alpha?}"
    },
    {
      "type": "eventHub",
      "name": "msgprod",
      "eventHubName": "%EVENTHUB_NAME_PROD%",
      "connection": "EVENTHUB_CONNECTION_STRING",
      "direction": "out"
    },
    {
      "type": "eventHub",
      "name": "msgstage",
      "eventHubName": "%EVENTHUB_NAME_STAGE%",
      "connection": "EVENTHUB_CONNECTION_STRING",
      "direction": "out"
    },
    {
      "name": "$return",
      "direction": "out",
      "type": "http"
    }
  ]
}
