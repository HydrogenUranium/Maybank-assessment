<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>artifactory</id>
            <username></username>
            <password></password>
        </server>
    </servers>

    <mirrors>
        <mirror>
            <id>artifactory-mirror</id>
            <name>DHL Artifactory</name>
            <url>https://artifactory.dhl.com/maven-remote</url>
            <mirrorOf>*,!discover-proj-prg-remote</mirrorOf>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>dhl-artifactory</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://artifactory.dhl.com/discover-proj-prg-remote</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <url>https://artifactory.dhl.com/discover-proj-prg-remote</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>dhl-artifactory</activeProfile>
    </activeProfiles>
</settings>