<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:deviceGroups="[mobile/groups/responsive]"
            cq:lastModified="{Date}2023-11-08T13:40:07.205+02:00"
            cq:lastModifiedBy="admin"
            cq:template="/conf/dhl/settings/wcm/templates/right-aligned-marketo-form"
            jcr:isCheckedOut="{Boolean}true"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="dhl/components/pages/editable-two-column-page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                layout="responsiveGrid">
            <headerv2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/dhl/language-masters/en-master/template-fragments/header/master"/>
            <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/breadcrumb"
                    startLevel="3"/>
            <two_columns_container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/two-columns-container-with-five-sections">
                <headline
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title_v2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/title-v2"
                            editable="{Boolean}true"/>
                </headline>
                <right-column-body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <marketoform
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/marketoForm"
                            editable="{Boolean}true"/>
                    <responsivegrid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"
                            editable="{Boolean}true">
                    </responsivegrid>
                </right-column-body>
                <description
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/text"
                            editable="{Boolean}true"/>
                </description>
                <left-column-body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <landingpoints
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/landingpoints"
                            editable="{Boolean}true"/>
                    <responsivegrid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"
                            editable="{Boolean}true">
                    </responsivegrid>
                </left-column-body>
                <bottom
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <cta_banner_gray
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/cta-banner-gray"
                            editable="{Boolean}true"/>
                </bottom>
            </two_columns_container>
            <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="{Boolean}true">
            </responsivegrid>
            <footerv2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/dhl/language-masters/en-master/template-fragments/footer/master"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                        jcr:primaryType="nt:unstructured"
                        title="Smaller Screen"
                        width="{Long}768"/>
                <tablet
                        jcr:primaryType="nt:unstructured"
                        title="Tablet"
                        width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
