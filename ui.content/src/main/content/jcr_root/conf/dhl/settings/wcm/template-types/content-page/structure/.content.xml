<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dhl/components/pages/editable-standard">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            layout="responsiveGrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/header">
                <cta_banner_with_poin
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/cta-banner-with-points"
                        style="sidebar"
                        type="businessAccount">
                    <analytics
                            jcr:primaryType="nt:unstructured"
                            interactionType="content"
                            name="'Open a Business Account' Banner"
                            position="Banner in the mobile header menu"
                            trackedInteractions="basic"/>
                </cta_banner_with_poin>
            </header>
            <main
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/page-container">
                <breadcrumb-container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/container"
                        editable="{Boolean}false">
                    <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/breadcrumb"
                            startLevel="3"/>
                </breadcrumb-container>
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/container"
                        editable="true"/>
            </main>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/footerV2"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
