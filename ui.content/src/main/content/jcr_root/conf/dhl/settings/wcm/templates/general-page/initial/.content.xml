<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/dhl/settings/wcm/templates/general-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dhl/components/pages/editable-standard">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            layout="responsiveGrid">
            <main
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/page-container">
                <hero_banner_v2
                        cq:isCancelledForChildren="{Boolean}true"
                        jcr:lastModified="{Date}2025-05-30T15:19:24.543+03:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/hero-banner-v2"
                        altValueFromPageImage="true"
                        displayPopupTitle="true"
                        imageFromPageImage="true"
                        isDecorative="false"
                        linkTarget="_self"
                        loop="true"
                        useVideo="false">
                </hero_banner_v2>
                <column_container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/column-container"
                        columns="one-column"
                        width="medium">
                    <first-container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid">
                        <responsivegrid
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid"/>
                    </first-container>
                </column_container>
            </main>
        </root>
    </jcr:content>
</jcr:root>
