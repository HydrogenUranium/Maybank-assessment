<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/dhl/settings/wcm/templates/landing-page-two-columns"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dhl/components/pages/editable-standard">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            layout="responsiveGrid">
            <headerv2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/headerV2">
                <cta_banner_with_poin
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/cta-banner-with-points"
                    style="sidebar"
                    type="businessAccount"/>
            </headerv2>
            <hero_banner
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/hero-banner"
                    editable="{Boolean}true"/>
            <top_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/column-container"
                columns="one-column"
                width="large">
                <first-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured"/>
                    </responsivegrid>
                </first-container>
                <second-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </top_container>
            <column_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/column-container"
                columns="two-columns"
                removeFirstChildTopMargin="true"
                topMargin="true"
                width="large">
                <first-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured"/>
                    </responsivegrid>
                </first-container>
                <second-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured"/>
                    </responsivegrid>
                </second-container>
            </column_container>
            <bottom_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/column-container"
                columns="one-column"
                width="large">
                <first-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured"/>
                    </responsivegrid>
                </first-container>
                <second-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </bottom_container>
            <footerv2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/footerV2"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
