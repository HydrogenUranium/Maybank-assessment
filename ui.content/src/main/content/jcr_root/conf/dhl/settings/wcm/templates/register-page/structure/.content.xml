<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/dhl/settings/wcm/templates/register-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dhl/components/pages/editable-standard">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            layout="responsiveGrid">
            <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/dhl/language-masters/en-master/template-fragments/header/master"/>
            <main
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/page-container">
                <hero_banner_v2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/hero-banner-v2"
                        editable="{Boolean}true"/>
                <column_container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/column-container"
                        columns="one-column"
                        width="medium">
                    <first-container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid">
                        <responsivegrid
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid"
                                editable="{Boolean}true">
                            <cq:responsive jcr:primaryType="nt:unstructured"/>
                        </responsivegrid>
                    </first-container>
                    <second-container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"/>
                </column_container>
            </main>
            <footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/dhl/language-masters/en-master/template-fragments/footer/master"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
