<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/dhl/settings/wcm/templates/error-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dhl/components/pages/editable-standard">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            layout="responsiveGrid">
            <main
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/page-container">
                <search
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/search"
                        title="Search Results"
                        descriptionFormat="Browse search results for &lt;b&gt;{0}&lt;/b&gt; (Showing {1} of {2} results)"
                        descriptionFormatNoResults="No results found for &lt;b&gt;{0}&lt;/b&gt;"
                        popularSearchesTitle="Popular searches"
                        sortByTitle="Sort By"
                        latestSortOptionTitle="Latest"
                        showMoreResultsButtonTitle="Show more results"/>
            </main>
        </root>
    </jcr:content>
</jcr:root>
