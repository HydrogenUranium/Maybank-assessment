<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/dhl/settings/wcm/templates/landing-page-two-columns"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dhl/components/pages/editable-standard">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/container"
                layout="responsiveGrid"/>
            <top_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/column-container"
                columns="two-columns"
                removeFirstChildTopMargin="true"
                topMargin="true"
                width="large">
                <first-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
                </first-container>
                <second-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
                </second-container>
            </top_container>
            <column_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/column-container"
                columns="one-column"
                width="large">
                <first-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
                </first-container>
            </column_container>
            <bottom_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/column-container"
                columns="one-column"
                width="large">
                <first-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
                </first-container>
            </bottom_container>
        </root>
    </jcr:content>
</jcr:root>
