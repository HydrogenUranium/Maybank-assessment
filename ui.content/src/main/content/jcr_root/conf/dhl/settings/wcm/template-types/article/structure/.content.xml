<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:deviceGroups="[mobile/groups/responsive]"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="dhl/components/pages/editable-article"
            cq:template="/conf/dhl/settings/wcm/templates/article">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                layout="responsiveGrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dhl/components/content/headerV2">
                <cta_banner_with_poin
                    jcr:primaryType="nt:unstructured"
                    type="businessAccount"
                    style="sidebar"
                    sling:resourceType="dhl/components/content/cta-banner-with-points"/>
            </header>
            <hero_image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-hero-image"/>
            <article_container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="classic"
                    useArticleStyles="true">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"
                            editable="true">
                    </responsivegrid>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container>
            <article_container_author_panel
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="classic"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <author_panel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-author-panel"/>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container_author_panel>
            <article_container_two
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="classic"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"
                            editable="true">
                    </responsivegrid>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container_two>
            <article_container_bottom
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="classic"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <social_static
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-social-static"/>
                    <content_banner
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-content-banner"/>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container_bottom>
            <similar_stories
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-similar-stories"/>
            <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="true"/>
            <footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/footerV2"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                        jcr:primaryType="nt:unstructured"
                        title="Smaller Screen"
                        width="{Long}768"/>
                <tablet
                        jcr:primaryType="nt:unstructured"
                        title="Tablet"
                        width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
