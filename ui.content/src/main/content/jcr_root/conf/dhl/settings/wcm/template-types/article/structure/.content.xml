<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:deviceGroups="[mobile/groups/responsive]"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="dhl/components/pages/editable-article"
            cq:template="/conf/dhl/settings/wcm/templates/article">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                layout="responsiveGrid">
            <headerv2
                    jcr:created="{Date}2023-10-06T14:26:41.371+03:00"
                    jcr:createdBy="admin"
                    jcr:lastModified="{Date}2023-10-06T14:26:41.371+03:00"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/headerV2">
                <cta_banner_with_poin
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dhl/components/content/cta-banner-with-points"
                        style="sidebar"
                        type="businessAccount">
                    <analytics
                            jcr:primaryType="nt:unstructured"
                            interactionType="content"
                            name="'Open a Business Account' Banner"
                            position="Banner in the mobile header menu"
                            trackedInteractions="basic"/>
                </cta_banner_with_poin>
            </headerv2>
            <article_header_container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="new"
                    showSidebar="false"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/breadcrumb"
                            startLevel="3"/>
                    <article-header
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-header"
                            editable="{Boolean}true"/>
                </body>
            </article_header_container>
            <article_container
                    jcr:lastModified="{Date}2023-10-06T14:22:14.716+03:00"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="new"
                    showSidebar="true"
                    useArticleStyles="true">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <article_author_banner
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-author-banner"
                            editable="{Boolean}true"/>
                    <hero_banner
                            jcr:created="{Date}2023-10-09T15:45:43.992+03:00"
                            jcr:createdBy="admin"
                            jcr:lastModified="{Date}2023-10-09T15:45:43.992+03:00"
                            jcr:lastModifiedBy="admin"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/hero-banner"
                            editable="{Boolean}true"/>
                    <responsivegrid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"
                            editable="true"/>
                    <cta_banner
                            jcr:created="{Date}2023-10-05T11:27:05.466+03:00"
                            jcr:createdBy="admin"
                            jcr:lastModified="{Date}2023-10-09T14:39:28.279+03:00"
                            jcr:lastModifiedBy="admin"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/cta-banner"
                            type="subscribeNewsletter"/>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <cta_banner_with_poin
                            jcr:created="{Date}2023-10-06T11:55:23.748+03:00"
                            jcr:createdBy="admin"
                            jcr:lastModified="{Date}2023-10-06T11:55:30.435+03:00"
                            jcr:lastModifiedBy="admin"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/cta-banner-with-points"
                            type="businessAccount"/>
                </sidebar>
            </article_container>
            <article_container_author_panel
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="new"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container_author_panel>
            <article_container_two
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="new"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <responsivegrid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid"
                            editable="true">
                    </responsivegrid>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container_two>
            <article_container_bottom
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="new"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <social_static
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-social-static"/>
                </body>
                <sidebar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </article_container_bottom>
            <article_footer_container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-container"
                    designMode="new"
                    showSidebar="false"
                    useArticleStyles="false">
                <body
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                    <article-footer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dhl/components/content/article-footer"
                            editable="{Boolean}true"/>
                </body>
            </article_footer_container>
            <similar_stories
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/article-similar-stories"/>
            <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="true"/>
            <footerv2
                    jcr:created="{Date}2023-10-06T14:26:54.120+03:00"
                    jcr:createdBy="admin"
                    jcr:lastModified="{Date}2023-10-06T14:26:54.120+03:00"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dhl/components/content/footerV2"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                        jcr:primaryType="nt:unstructured"
                        title="Smaller Screen"
                        width="{Long}768"/>
                <tablet
                        jcr:primaryType="nt:unstructured"
                        title="Tablet"
                        width="{Long}992"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
