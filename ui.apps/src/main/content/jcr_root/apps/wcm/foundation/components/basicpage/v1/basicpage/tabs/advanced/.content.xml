<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured">
    <items jcr:primaryType="nt:unstructured">
        <column jcr:primaryType="nt:unstructured">
            <items jcr:primaryType="nt:unstructured">
                <seo jcr:primaryType="nt:unstructured">
                    <items jcr:primaryType="nt:unstructured">
                        <canonicalUrl jcr:primaryType="nt:unstructured"/>
                        <robotsTags jcr:primaryType="nt:unstructured"/>
                        <sitemapChangefreq jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldLabel="Change Frequency"
                            emptyText="Select a specific 'changefreq' Sitemap value for this page"
                            fieldDescription="Select a specific 'changefreq' Sitemap value for this page. The default value for all pages is set in OSGi Config 'DHL Discovery - Page Tree Sitemap Generator'"
                            emptyOption="{Boolean}true"
                            required="{Boolean}false"
                            name="./sitemapChangefreq">
                            <items jcr:primaryType="nt:unstructured">
                                <always jcr:primaryType="nt:unstructured"
                                    text="always"
                                    value="always"/>
                                <hourly jcr:primaryType="nt:unstructured"
                                    text="hourly"
                                    value="hourly"/>
                                <daily jcr:primaryType="nt:unstructured"
                                    text="daily"
                                    value="daily"/>
                                <weekly jcr:primaryType="nt:unstructured"
                                    text="weekly"
                                    value="weekly"/>
                                <monthly jcr:primaryType="nt:unstructured"
                                    text="monthly"
                                    value="monthly"/>
                                <yearly jcr:primaryType="nt:unstructured"
                                    text="yearly"
                                    value="yearly"/>
                                <never jcr:primaryType="nt:unstructured"
                                    text="never"
                                    value="never"/>
                            </items>
                        </sitemapChangefreq>
                        <sitemapPriority jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldLabel="Priority"
                            emptyText="Select a specific 'priority' Sitemap value for this page"
                            fieldDescription="Select a specific 'priority' Sitemap value for this page. The default value for all pages is set in OSGi Config 'DHL Discovery - Page Tree Sitemap Generator'"
                            emptyOption="{Boolean}true"
                            required="{Boolean}false"
                            name="./sitemapPriority">
                            <items jcr:primaryType="nt:unstructured">
                                <priority_1.0 jcr:primaryType="nt:unstructured"
                                    text="1.0"
                                    value="1.0"/>
                                <priority_0.9 jcr:primaryType="nt:unstructured"
                                    text="0.9"
                                    value="0.9"/>
                                <priority_0.8 jcr:primaryType="nt:unstructured"
                                    text="0.8"
                                    value="0.8"/>
                                <priority_0.7 jcr:primaryType="nt:unstructured"
                                    text="0.7"
                                    value="0.7"/>
                                <priority_0.6 jcr:primaryType="nt:unstructured"
                                    text="0.6"
                                    value="0.6"/>
                                <priority_0.5 jcr:primaryType="nt:unstructured"
                                    text="0.5"
                                    value="0.5"/>
                                <priority_0.4 jcr:primaryType="nt:unstructured"
                                    text="0.4"
                                    value="0.4"/>
                                <priority_0.3 jcr:primaryType="nt:unstructured"
                                    text="0.3"
                                    value="0.3"/>
                                <priority_0.2 jcr:primaryType="nt:unstructured"
                                    text="0.2"
                                    value="0.2"/>
                                <priority_0.1 jcr:primaryType="nt:unstructured"
                                    text="0.1"
                                    value="0.1"/>
                            </items>
                        </sitemapPriority>
                        <sitemapPriorityTypeHint jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            ignoreData="{Boolean}true"
                            value="Double"
                            name="./sitemapPriority@TypeHint"/>
                        <sitemapRoot jcr:primaryType="nt:unstructured"/>
                    </items>
                </seo>
            </items>
        </column>
    </items>
</jcr:root>
