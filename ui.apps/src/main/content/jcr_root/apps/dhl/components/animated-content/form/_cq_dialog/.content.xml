<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Properties"
          sling:resourceType="cq/gui/components/authoring/dialog" xmlns:granite="http://www.adobe.com/jcr/granite/1.0">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="Title"
                            name="./title"/>
                    <description
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                            useFixedInlineToolbar="{Boolean}true"
                            fieldLabel="Description"
                            name="./description"/>
                    <linkText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="Link Text"
                            name="./linkText"/>
                    <formTitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="Form Title"
                            name="./formTitle"/>
                    <accordion
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/accordion"
                            multiple="{Boolean}true" variant="quiet" margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <firstName
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="First Name"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./firstNamePlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./firstNameMessage"/>
                                </items>
                            </firstName>
                            <lastName
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Last Name"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./lastNamePlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./lastNameMessage"/>
                                </items>
                            </lastName>
                            <email
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Email"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./emailPlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./emailMessage"/>
                                </items>
                            </email>
                            <companyName
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Company Name"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./companyNamePlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./companyNameMessage"/>
                                </items>
                            </companyName>
                            <phoneNumber
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Phone Number"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./phoneNumberPlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./phoneNumberMessage"/>
                                </items>
                            </phoneNumber>
                            <address
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Address"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./addressPlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./addressMessage"/>
                                </items>
                            </address>
                            <zipOrPostcode
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Zip or Postcode"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./zipOrPostcodePlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./zipOrPostcodeMessage"/>
                                </items>
                            </zipOrPostcode>
                            <city
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="City"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./cityPlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./cityMessage"/>
                                </items>
                            </city>
                            <country
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Country"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <placeholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Placeholder"
                                            name="./countryPlaceholder"/>
                                    <message
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                            fieldLabel="Validation Message"
                                            name="./countryMessage"/>
                                </items>
                            </country>
                        </items>
                    </accordion>
                    <nextButtonTitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="'Next' button title"
                            name="./nextButtonTitle"/>
                    <applyNowTitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="'Apply Now' button title"
                            name="./applyNowTitle"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
