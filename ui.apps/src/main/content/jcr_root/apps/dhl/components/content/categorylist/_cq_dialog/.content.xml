                        name="./path2"
                        />
                    <path3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Category 3"
                        rootPath="/content/dhl"
                        name="./path3"
                        />
                        
                    <path4
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Category 4"
                        rootPath="/content/dhl"
                        name="./path4"
                        />
                    <path5
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Category 5"
                        rootPath="/content/dhl"
                        name="./path5"
                        />
                    <path6
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Category 6"
                        rootPath="/content/dhl"
                        name="./path6"
                        />
                </items>
            </column>
        </items>
    </content>
</jcr:root>
