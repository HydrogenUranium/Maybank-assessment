<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Properties"
          sling:resourceType="cq/gui/components/authoring/dialog" xmlns:granite="http://www.adobe.com/jcr/granite/1.0">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <type
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldLabel="Type"
                            granite:class="cq-dialog-dropdown-multiple-showhide"
                            name="./type">
                        <granite:data
                                jcr:primaryType="nt:unstructured"
                                cq-dialog-dropdown-showhide-target=".hero-banner-options-showhide"/>
                        <items
                                jcr:primaryType="nt:unstructured">
                            <cosmetics
                                    jcr:primaryType="nt:unstructured"
                                    text="Cosmetics"
                                    value="cosmetics"/>
                            <consumerElectronics
                                    jcr:primaryType="nt:unstructured"
                                    text="Consumer Electronics"
                                    value="consumerElectronics"/>
                            <fastFashion
                                    jcr:primaryType="nt:unstructured"
                                    text="Fast Fashion"
                                    value="fastFashion"/>
                            <luxuryGoods
                                    jcr:primaryType="nt:unstructured"
                                    text="Luxury Goods"
                                    value="luxuryGoods"/>
                            <medicalDevices
                                    jcr:primaryType="nt:unstructured"
                                    text="Medical Devices"
                                    value="medicalDevices"/>
                            <musicalInstruments
                                    jcr:primaryType="nt:unstructured"
                                    text="Musical Instruments"
                                    value="musicalInstruments"/>
                            <officeSupplies
                                    jcr:primaryType="nt:unstructured"
                                    text="Office Supplies"
                                    value="officeSupplies"/>
                            <packagingMarketingOpportunities
                                    jcr:primaryType="nt:unstructured"
                                    text="Packaging Marketing Opportunities"
                                    value="packagingMarketingOpportunities"/>
                            <sportingGoods
                                    jcr:primaryType="nt:unstructured"
                                    text="Sporting Goods"
                                    value="sportingGoods"/>
                            <textiles
                                    jcr:primaryType="nt:unstructured"
                                    text="Textiles"
                                    value="textiles"/>
                        </items>
                    </type>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Title"
                        name="./title"/>
                    <description
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                        useFixedInlineToolbar="{Boolean}true"
                        fieldLabel="Description"
                        name="./description"/>
                    <cosmeticsImgAltText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            granite:class="hero-banner-options-showhide">
                        <granite:data
                                jcr:primaryType="nt:unstructured"
                                showhidetargetvalue="cosmetics"/>
                        <items jcr:primaryType="nt:unstructured">
                            <imgAltText1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Hero-banner Image Alt Text"
                                    name="./imgAltText1"
                                    fieldDescription="Set Hero-banner image alt text for SEO"
                                    emptyText="Hero-banner Image Alt Text Placeholder"/>
                        </items>
                    </cosmeticsImgAltText>
                    <consumerElectronicsImgAltText
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
