<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    granite:class="cmp-embeddable-infogram__editor"
    jcr:primaryType="nt:unstructured"
    jcr:title="Infogram"
    sling:resourceType="granite/ui/components/coral/foundation/container">
    <items jcr:primaryType="nt:unstructured">
        <wordPressEmbedButton
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/button"
                granite:class="cq-dialog-paste-button"
                fieldLabel="Button"
                text="Paste"
                icon="pasteText"
                type="button">
            <granite:data
                    jcr:primaryType="nt:unstructured"
                    paste-target="word-press-embed-field"/>
        </wordPressEmbedButton>
        <wordPressEmbedField
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                rows="2"
                granite:class="word-press-embed-field"
                fieldLabel="Word Press Embed"
                validation="word-press-embed"
                name="./wordPressEmbedField"
                required="{Boolean}true"/>
    </items>
    <granite:data
            jcr:primaryType="nt:unstructured"
            cmp-embed-dialog-edit-embeddableoptions="true"
            cmp-embed-dialog-edit-showhidetargetvalue="dhl/components/content/embed/embeddable/infogram"/>
</jcr:root>
