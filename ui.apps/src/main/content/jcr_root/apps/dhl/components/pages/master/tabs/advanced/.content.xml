<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured">
    <items jcr:primaryType="nt:unstructured">
        <column jcr:primaryType="nt:unstructured">
            <items jcr:primaryType="nt:unstructured">
                <seo jcr:primaryType="nt:unstructured">
                    <items jcr:primaryType="nt:unstructured">
                        <canonicalUrl jcr:primaryType="nt:unstructured"/>
                        <robotsTags jcr:primaryType="nt:unstructured"/>
                        <noIndexRobotsTagsInherit
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            renderReadOnly="{Boolean}true"
                            text="Inherit 'noindex' value of 'Robots Tags' property in Sitemap for child pages"
                            name="./noIndexRobotsTagsInherit"
                            value="{Boolean}true">
                        <granite:data
                                jcr:primaryType="nt:unstructured"
                                cq-msm-lockable="./noIndexRobotsTagsInherit"/>
                        </noIndexRobotsTagsInherit>
                        <sitemapChangefreq
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            emptyOption="{Boolean}true"
                            emptyText="Select a specific 'changefreq' Sitemap value for this page"
                            fieldDescription="Select a specific 'changefreq' Sitemap value for this page. The default value for all pages is set in OSGi Config 'DHL Discovery - Page Tree Sitemap Generator'"
                            fieldLabel="Change Frequency"
                            name="./sitemapChangefreq"
                            required="{Boolean}false">
                            <items jcr:primaryType="nt:unstructured">
                                <always
                                    jcr:primaryType="nt:unstructured"
                                    text="always"
                                    value="always"/>
                                <hourly
                                    jcr:primaryType="nt:unstructured"
                                    text="hourly"
                                    value="hourly"/>
                                <daily
                                    jcr:primaryType="nt:unstructured"
                                    text="daily"
                                    value="daily"/>
                                <weekly
                                    jcr:primaryType="nt:unstructured"
                                    text="weekly"
                                    value="weekly"/>
                                <monthly
                                    jcr:primaryType="nt:unstructured"
                                    text="monthly"
                                    value="monthly"/>
                                <yearly
                                    jcr:primaryType="nt:unstructured"
                                    text="yearly"
                                    value="yearly"/>
                                <never
                                    jcr:primaryType="nt:unstructured"
                                    text="never"
                                    value="never"/>
                            </items>
                            <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    cq-msm-lockable="./sitemapChangefreq"/>
                        </sitemapChangefreq>
                        <sitemapChangefreqInherit
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            renderReadOnly="{Boolean}true"
                            text="Inherit 'Change Frequency' property for child pages"
                            name="./sitemapChangefreqInherit"
                            value="{Boolean}true">
                        <granite:data
                                jcr:primaryType="nt:unstructured"
                                cq-msm-lockable="./sitemapChangefreqInherit"/>
                        </sitemapChangefreqInherit>
                        <sitemapChangefreqInheritTypeHint
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            ignoreData="{Boolean}true"
                            name="./sitemapChangefreqInherit@TypeHint"
                            value="Boolean"/>
                        <sitemapPriority
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            emptyOption="{Boolean}true"
                            emptyText="Select a specific 'priority' Sitemap value for this page"
                            fieldDescription="Select a specific 'priority' Sitemap value for this page. The default value for all pages is set in OSGi Config 'DHL Discovery - Page Tree Sitemap Generator'"
                            fieldLabel="Priority"
                            name="./sitemapPriority"
                            required="{Boolean}false">
                            <items jcr:primaryType="nt:unstructured">
                                <priority_1.0
                                    jcr:primaryType="nt:unstructured"
                                    text="1.0"
                                    value="1.0"/>
                                <priority_0.9
                                    jcr:primaryType="nt:unstructured"
                                    text="0.9"
                                    value="0.9"/>
                                <priority_0.8
                                    jcr:primaryType="nt:unstructured"
                                    text="0.8"
                                    value="0.8"/>
                                <priority_0.7
                                    jcr:primaryType="nt:unstructured"
                                    text="0.7"
                                    value="0.7"/>
                                <priority_0.6
                                    jcr:primaryType="nt:unstructured"
                                    text="0.6"
                                    value="0.6"/>
                                <priority_0.5
                                    jcr:primaryType="nt:unstructured"
                                    text="0.5"
                                    value="0.5"/>
                                <priority_0.4
                                    jcr:primaryType="nt:unstructured"
                                    text="0.4"
                                    value="0.4"/>
                                <priority_0.3
                                    jcr:primaryType="nt:unstructured"
                                    text="0.3"
                                    value="0.3"/>
                                <priority_0.2
                                    jcr:primaryType="nt:unstructured"
                                    text="0.2"
                                    value="0.2"/>
                                <priority_0.1
                                    jcr:primaryType="nt:unstructured"
                                    text="0.1"
                                    value="0.1"/>
                            </items>
                            <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    cq-msm-lockable="./sitemapPriority"/>
                        </sitemapPriority>
                        <sitemapPriorityTypeHint
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            ignoreData="{Boolean}true"
                            name="./sitemapPriority@TypeHint"
                            value="Double"/>
                        <sitemapPriorityInherit
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            renderReadOnly="{Boolean}true"
                            text="Inherit 'Priority' property for child pages"
                            name="./sitemapPriorityInherit"
                            value="{Boolean}true">
                        <granite:data
                                jcr:primaryType="nt:unstructured"
                                cq-msm-lockable="./sitemapPriorityInherit"/>
                        </sitemapPriorityInherit>
                        <sitemapPriorityInheritTypeHint
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            ignoreData="{Boolean}true"
                            name="./sitemapPriorityInherit@TypeHint"
                            value="Boolean"/>
                        <sitemapRoot jcr:primaryType="nt:unstructured"/>
                    </items>
                </seo>
            </items>
        </column>
    </items>
</jcr:root>
