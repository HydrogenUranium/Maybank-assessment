<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Download asset properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <asset
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Asset"
                    sling:resourceType="granite/ui/components/foundation/section">
                <items jcr:primaryType="nt:unstructured">
                    <column jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="Title"
                                    name="./title"/>
                            <brief
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textarea"
                                    fieldLabel="Brief Description"
                                    name="./brief"/>
                            <assetimage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Asset Image"
                                    rootPath="/content/dam"
                                    name="./assetimage"/>

                            <ctatitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="CTA Title - (non-Marketo form only)"
                                    name="./ctatitle"/>
                            <ctatitlesample
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="CTA Title (sample) - (non-Marketo form only)"
                                    name="./ctatitlesample"/>

                            <assetpath
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Asset Path - (non-Marketo form only)"
                                    rootPath="/content/dam"
                                    name="./assetpath"/>
                            <assetpathsample
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Asset Path (sample) - (non-Marketo form only)"
                                    rootPath="/content/dam"
                                    name="./assetpathsample"/>
                        </items>
                    </column>
                </items>
            </asset>
            <marketo
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Marketo"
                    sling:resourceType="granite/ui/components/foundation/section">
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <marketoformid
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="Marketo FormID"
                                    name="./marketoFormId"
                                    value="1796"
                                    emptyText="Marketo form ID of the 'hidden' form, such as 1795"
                                    fieldDescription="Form (asset) ID of the form in Marketo"/>
                            <marketomunchkinid
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="Marketo Munchkin ID"
                                    name="./marketoMunchkinId"
                                    value="903-EZK-832"
                                    emptyText="Marketo project ID of the 'hidden' form, such as 903-EZK-832"
                                    fieldDescription="Marketo project (munchkin ID)"/>
                            <hiddenmarketoformid
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="HIDDEN Marketo FormID"
                                    name="./hiddenMarketoId"
                                    value="1724"
                                    emptyText="Marketo form ID of the 'hidden' form, such as 1799"
                                    fieldDescription="Form (asset) ID of the form in Marketo. Refers to 'hidden' form."/>
                            <hiddenmarketomunchkinid
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="HIDDEN Marketo Munchkin ID"
                                    name="./hiddenMarketoMunchkinId"
                                    value="903-EZK-832"
                                    emptyText="Marketo project ID of the 'hidden' form, such as 903-EZK-832"
                                    fieldDescription="Marketo project (munchkin ID). Refers to 'hidden' form."/>
                            <marketoHostname
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldLabel="Marketo Host"
                                    name="./marketoHostname"
                                    value="https://express-resource.dhl.com"
                                    emptyText="Marketo hostname, such as https://express-resource.dhl.com"
                                    fieldDescription="Marketo hostname. Controls where the form submissions are sent."/>
                        </items>
                    </column>
                </items>
            </marketo>
        </items>
    </content>
</jcr:root>
